{
  "name": "vue-i18n-ally",
  "displayName": "Vue i18n Ally",
  "description": "Forked from think2011/vscode-vue-i18n, with some feature changes",
  "version": "0.6.0",
  "publisher": "antfu",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [
    "workspaceContains:package.json",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "keywords": [
    "vue",
    "vue-i18n",
    "vue i18n",
    "i18n"
  ],
  "icon": "static/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-vue-i18n-ally"
  },
  "homepage": "https://github.com/antfu/vscode-vue-i18n-ally",
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.vue-i18n-ally.config-locales",
        "title": "Vue i18n Ally: Manual configure locales path"
      },
      {
        "command": "extension.vue-i18n-ally.config-locales-auto",
        "title": "Vue i18n Ally: Auto detect locales path"
      },
      {
        "command": "extension.vue-i18n-ally.config-display-language",
        "title": "Vue i18n Ally: Change display language"
      },
      {
        "command": "extension.vue-i18n-ally.debug",
        "title": "Vue i18n Ally: Debug"
      },
      {
        "command": "extension.vue-i18n-ally.copy-key",
        "title": "Copy i18n key",
        "icon": "./static/icon-copy.svg"
      },
      {
        "command": "extension.vue-i18n-ally.translate-key",
        "title": "Translate",
        "icon": "./static/icon-translate.svg"
      },
      {
        "command": "extension.vue-i18n-ally.edit-key",
        "title": "Edit",
        "icon": "./static/icon-edit.svg"
      },
      {
        "command": "extension.vue-i18n-ally.open-key",
        "title": "Open locale file",
        "icon": "./static/icon-open-file.svg"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "extension.vue-i18n-ally.open-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /openable/",
          "group": "inline@1"
        },
        {
          "command": "extension.vue-i18n-ally.translate-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /translatable/",
          "group": "inline@2"
        },
        {
          "command": "extension.vue-i18n-ally.copy-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /node/",
          "group": "inline@3"
        },
        {
          "command": "extension.vue-i18n-ally.edit-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /record/",
          "group": "inline@4"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vue-i18n-ally",
          "title": "Vue i18n Ally",
          "icon": "./static/logo-view-container.svg"
        }
      ]
    },
    "views": {
      "vue-i18n-ally": [
        {
          "id": "locales-tree-file",
          "name": "File",
          "when": "vue-i18n-ally-enabled"
        },
        {
          "id": "locales-progress",
          "name": "Progress",
          "when": "vue-i18n-ally-enabled"
        },
        {
          "id": "locales-tree",
          "name": "Tree",
          "when": "vue-i18n-ally-enabled"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vue i18n Ally",
      "properties": {
        "vue-i18n-ally.localesPaths": {
          "type": "string",
          "default": "",
          "description": "Path to locales directory (relative to project root)"
        },
        "vue-i18n-ally.sourceLanguage": {
          "type": "string",
          "default": "en",
          "description": "Source language for machine translation"
        },
        "vue-i18n-ally.displayLanguage": {
          "type": "string",
          "default": "en",
          "description": "Default display language"
        }
      }
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "‚ö°Features"
      },
      {
        "type": "fix",
        "section": "üêûBug Fixes"
      },
      {
        "type": "test",
        "section": "‚öóTests"
      },
      {
        "type": "docs",
        "section": "üìöDocumentations"
      },
      {
        "type": "ci",
        "hidden": true
      }
    ]
  },
  "scripts": {
    "release": "standard-version",
    "publish": "standard-version && vsce publish",
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.4.5",
    "vscode": "^1.1.33",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "clipboardy": "^2.0.0",
    "fast-glob": "^2.2.6",
    "flat": "^4.1.0",
    "limax": "^1.7.0",
    "lodash": "^4.17.11",
    "translation.js": "^0.7.8"
  }
}
