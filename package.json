{
  "name": "vue-i18n",
  "displayName": "Vue i18n",
  "description": "vue i18n",
  "version": "0.0.1",
  "publisher": "vue-i18n",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vue-i18n",
      "properties": {
        "vue-i18n.transPaths": {
          "type": "array",
          "default": [
            "./crm/src/locales",
            "./sales/src/locales"
          ],
          "description": "翻译文件的目录（相对于根目录）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
